{
    presets: [
        [
            "env", 
            {
                "targets": {
                    "browsers": ["last 2 versions"]
                }
            }
        ]
    ],
    plugins: [
        "babel-plugin-transform-async-to-generator",
        "transform-object-rest-spread",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true,
        }]
    ]
}
